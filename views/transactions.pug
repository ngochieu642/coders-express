div
    a(href="/books/create") Admin Books Page
    span &nbsp
    a(href="/users/create") Admin Users Page
    span &nbsp
    a(href="/transactions/create") Admin Transactions Page

h2 Transactions List

div
  ol
    each transaction in transactions
      li
        div
          a(href='/transactions/' + transaction.id)= transaction.id
          span
            p= "Is Complete: " + transaction.isComplete
            a(href='/transactions/' + transaction.id + '/complete') Complete
        div
          span= "User ID: " + transaction.userId
          span &nbsp
          a(href='/users/' + transaction.userId) View
        div
          span= "Book ID: " + transaction.bookId
          span &nbsp
          a(href='/books/' + transaction.bookId) View